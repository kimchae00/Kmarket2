<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.kmarket2.dao.TermsPolicyDAO">
	<select id="selectCate1" resultType="kr.co.kmarket2.vo.ProdCate1VO">
		select * from `km_product_cate1`;
	</select>
	<select id="selectCate2" resultType="kr.co.kmarket2.vo.ProdCate2VO">
		select * from `km_product_cate2`;
	</select>
	
	<select id="selectTermsBuyer" resultType="kr.co.kmarket2.vo.TermsBuyerVO">
		select * from `km_terms_policy_buyer`;
	</select>
	
	<select id="selectTermsFinance" resultType="kr.co.kmarket2.vo.TermsFinanceVO">
		select * from `km_terms_policy_finance`;
	</select>
	
	<select id="selectTermsLocation" resultType="kr.co.kmarket2.vo.TermsLocationVO">
		select * from `km_terms_policy_location`;
	</select>
	
	<select id="selectTermsPrivacy" resultType="kr.co.kmarket2.vo.TermsPrivacyVO">
		select * from `km_terms_policy_privacy`;
	</select>

	<select id="selectTermsSeller" resultType="kr.co.kmarket2.vo.TermsSellerVO">
		select * from `km_terms_policy_seller`;
	</select>	
	
	
	
	
</mapper>